{
    "name": "url-param-compressor",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/NaverPayDev/pie/tree/main/packages/url-compressor"
    },
    "bugs": {
        "url": "https://github.com/NaverPayDev/pie/issues"
    },
    "homepage": "https://naverpaydev.github.io/pie/docs/docs/@naverpay/url-compressor/",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.mjs",
    "types": "./dist/cjs/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.mts",
                "default": "./dist/esm/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        },
        "./package.json": "./package.json"
    },
    "keywords": [
        "naver",
        "naverpay",
        "naver financial",
        "url",
        "compressor"
    ],
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "test": "vitest --watch=false --reporter=default",
        "build": "pnpm run clean && vite build"
    },
    "devDependencies": {
        "vitest": "^3.1.1"
    },
    "dependencies": {
        "fflate": "^0.8.2"
    }
}
