{
    "name": "@naverpay/svg-manager",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/NaverPayDev/pie/tree/main/packages/svg-manager"
    },
    "bugs": {
        "url": "https://github.com/NaverPayDev/pie/issues"
    },
    "homepage": "https://naverpaydev.github.io/pie/docs/@naverpay/svg-manager/",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.mjs",
    "types": "./dist/cjs/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.mts",
                "default": "./dist/esm/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        },
        "./package.json": "./package.json"
    },
    "sideEffects": false,
    "keywords": [
        "naver",
        "naverpay",
        "svg",
        "svg-manager"
    ],
    "author": "@NaverPayDev/frontend",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "npm run clean && vite build"
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/react": "catalog:",
        "csstype": "^3.1.3"
    },
    "peerDependencies": {
        "react": "catalog:",
        "react-dom": "catalog:"
    },
    "license": "MIT"
}
