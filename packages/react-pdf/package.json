    "devDependencies": {
        "@naverpay/rollup": "workspace:^",
        "@naverpay/tsconfig": "workspace:^",
        "@types/node": "^20.12.7",
        "@types/react": "^18.2.75",
        "rollup": "^2.79.1"
    },
    "peerDependencies": {
        "react": "^17 || ^18"
    }
}
