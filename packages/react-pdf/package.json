{
    "name": "@naverpay/react-pdf",
    "version": "0.0.1",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.mjs",
    "types": "./dist/cjs/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.mjs",
            "types": "./dist/cjs/index.d.ts"
        },
        "./index.css": "./dist/cjs/index.css",
        "./package.json": "./package.json"
    },
    "keywords": [
        "naver",
        "naverpay",
        "naver financial",
        "pdf",
        "react-pdf"
    ],
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "build-declarations": "tsc --declarationDir dist/cjs && tsc --declarationDir dist/esm",
        "build": "pnpm run clean && pnpm run build-declarations && rollup -c"
    },
    "dependencies": {
        "intersection-observer": "^0.12.2",
        "classnames": "^2.5.1"
    },
    "devDependencies": {
        "rollup": "^2.79.1",
        "core-js": "^3.29.0",
        "@naverpay/tsconfig": "workspace:^",
        "@naverpay/rollup": "workspace:^",
        "@types/react": "^18.2.75",
        "@types/node": "^20.12.7"
    },
    "peerDependencies": {
        "react": "^17 || ^18"
    }
}
