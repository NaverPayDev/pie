{
    "name": "@naverpay/react-pdf",
    "version": "0.3.4",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "keywords": [
        "naver",
        "naverpay",
        "naver financial",
        "pdf",
        "react-pdf"
    ],
    "files": [
        "dist"
