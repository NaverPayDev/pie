{
    "name": "@naverpay/react-pdf",
    "version": "0.2.1",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "keywords": [
        "naver",
        "naverpay",
        "naver financial",
        "pdf",
        "react-pdf"
    ],
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "build-declarations": "tsc --declarationDir dist",
        "build": "pnpm run clean && pnpm run build-declarations && rollup -c",
        "start": "rollup -c --watch"
    },
    "dependencies": {
        "intersection-observer": "^0.12.2",
        "classnames": "^2.5.1"
    },
    "devDependencies": {
        "rollup": "^2.79.1",
        "core-js": "^3.29.0",
        "@naverpay/tsconfig": "workspace:^",
        "@naverpay/rollup": "workspace:^",
        "@types/react": "^18.2.75",
        "@types/node": "^20.12.7"
    },
    "peerDependencies": {
        "react": "^17 || ^18"
    },
    "size-limit": {
        "import": "{ PdfViewer }",
        "limit": "300 kB"
    }
}
