{
    "name": "@naverpay/utils",
    "description": "javascript utility library",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/NaverPayDev/pie/tree/main/packages/utils"
    },
    "bugs": {
        "url": "https://github.com/NaverPayDev/pie/issues"
    },
    "homepage": "https://naverpaydev.github.io/pie/docs/docs/@naverpay/utils/",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.mjs",
    "types": "./dist/cjs/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.mts",
                "default": "./dist/esm/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        },
        "./package.json": "./package.json"
    },
    "sideEffects": false,
    "scripts": {
        "clean": "rm -rf dist",
        "build": "npm run clean && vite build",
        "test": "vitest --watch=false --reporter=default"
    },
    "keywords": [
        "javascript",
        "utils",
        "naver",
        "naverpay"
    ],
    "devDependencies": {
        "@types/node": "catalog:",
        "happy-dom": "^17.4.4",
        "vitest": "^3.1.1"
    },
    "files": [
        "dist"
    ]
}
