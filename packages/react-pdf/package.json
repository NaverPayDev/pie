{
    "name": "@naverpay/react-pdf",
    "version": "1.0.4",
    "repository": {
        "type": "git",
        "url": "https://github.com/NaverPayDev/pie/tree/main/packages/react-pdf"
    },
    "bugs": {
        "url": "https://github.com/NaverPayDev/pie/issues"
    },
    "homepage": "https://naverpaydev.github.io/pie/docs/docs/@naverpay/react-pdf/",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.mjs",
    "types": "./dist/cjs/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.mts",
                "default": "./dist/esm/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        },
        "./style.css": {
            "import": "./dist/esm/style.css",
            "require": "./dist/cjs/style.css"
        },
        "./package.json": "./package.json"
    },
    "keywords": [
        "naver",
        "naverpay",
        "naver financial",
        "pdf",
        "react-pdf"
    ],
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "build": "pnpm run clean && vite build"
    },
    "dependencies": {
        "classnames": "^2.5.1",
        "pdfjs-dist": "^4.10.38"
    },
    "devDependencies": {
        "@types/node": "^20.12.7",
        "@types/react": "^18.2.75"
    },
    "peerDependencies": {
        "react": "^17 || ^18 || ^19"
    }
}
