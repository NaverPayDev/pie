{
    "name": "@naverpay/svg-manager",
    "version": "1.0.0",
    "description": "A package for managing SVG assets in react projects",
    "keywords": [
        "naver",
        "naverpay",
        "svg",
        "svg-manager"
    ],
    "homepage": "https://naverpaydev.github.io/pie/docs/docs/@naverpay/svg-manager/",
    "bugs": {
        "url": "https://github.com/NaverPayDev/pie/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/NaverPayDev/pie/tree/main/packages/svg-manager"
    },
    "license": "MIT",
    "author": "@NaverPayDev/frontend",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.mts",
                "default": "./dist/esm/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        },
        "./package.json": "./package.json",
        "./react17": {
            "import": {
                "types": "./dist/react17/esm/index.d.mts",
                "default": "./dist/react17/esm/index.mjs"
            },
            "require": {
                "types": "./dist/react17/cjs/index.d.ts",
                "default": "./dist/react17/cjs/index.js"
            }
        }
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.mjs",
    "types": "./dist/cjs/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npm run clean && pnpm build:react17 && pnpm build:react18",
        "build:react17": "vite build --config vite.config.react17.mjs && node ./tsup-executor.mjs",
        "build:react18": "vite build",
        "clean": "rm -rf dist"
    },
    "devDependencies": {
        "@types/react": "catalog:",
        "@types/react-dom": "catalog:",
        "@vitejs/plugin-react": "^4.5.2",
        "csstype": "^3.1.3",
        "react": "catalog:",
        "react-dom": "catalog:"
    },
    "peerDependencies": {
        "react": "catalog:",
        "react-dom": "catalog:"
    }
}
