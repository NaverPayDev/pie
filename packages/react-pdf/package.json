{
    "name": "@naverpay/react-pdf",
    "version": "1.1.0",
    "description": "A React component for rendering PDF documents",
    "keywords": [
        "naver",
        "naverpay",
        "naver financial",
        "pdf",
        "react-pdf"
    ],
    "homepage": "https://naverpaydev.github.io/pie/docs/docs/@naverpay/react-pdf/",
    "bugs": {
        "url": "https://github.com/NaverPayDev/pie/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/NaverPayDev/pie/tree/main/packages/react-pdf"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.mts",
                "default": "./dist/esm/index.mjs"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        },
        "./package.json": "./package.json",
        "./style.css": {
            "import": "./dist/esm/style.css",
            "require": "./dist/cjs/style.css"
        }
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.mjs",
    "types": "./dist/cjs/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm run clean && vite build",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "classnames": "^2.5.1",
        "pdfjs-dist": "^4.10.38"
    },
    "devDependencies": {
        "@types/node": "catalog:",
        "@types/react": "catalog:"
    },
    "peerDependencies": {
        "react": "catalog:"
    }
}
