{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {"cache": false, "outputs": ["dist/**"]},
        "lint": {"cache": false},
        "lint:fix": {"cache": false},
        "prettier": {"cache": false},
        "prettier:fix": {"cache": false},
        "test": {"cache": false},
        "clean": {"cache": false},
        "docs#build:docs": {
            "outputs": ["dist/**"]
        },
        "ui-docs#build:docs": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "docs#start": {},
        "ui-docs#start": {
            "dependsOn": ["^build"]
        }
    }
}
